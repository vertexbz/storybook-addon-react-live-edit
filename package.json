  "description": "storybook.js addon providing live react story editing and preview",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && babel src --ignore src/__tests__,src/__mocks__ --out-dir dist -s",
    "lint": "eslint src --ext .js --cache",
