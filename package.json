    "@storybook/components": "^3.4.11",
    "@storybook/react": "^3.4.11",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-codegen": "^3.0.0",
